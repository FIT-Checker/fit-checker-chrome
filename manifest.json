{
	"name": "FIT Checker",
	"version": "0.6.5",
	"manifest_version": 2,
	"description": "Rozšíření pro studenty FIT ČVUT. Slouží ke kontrole aktuálního stavu hodnocení v předmětech na serveru edux.fit.cvut.cz.",
	"icons": {
		"16": "components/common-assets/img/icon-16.png",
		"48": "components/common-assets/img/icon-48.png",
		"128": "components/common-assets/img/icon-128.png"
	},
	"browser_action": {
		"default_icon": "components/common-assets/img/icon-16.png",
		"default_title": "FIT Checker",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"https://edux.fit.cvut.cz/*"
	]
}
